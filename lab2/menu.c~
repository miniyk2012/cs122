#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main() {
    char cmd[128];	
    while(1)
    {	
		scanf("%s", cmd);
        if(strcmp(cmd, "help") == 0) {
			printf("this is help cmd\n");
		}
		else if (strcmp(cmd, "quit") == 0) {
			exit(0);				
		}
		else {
			printf("this is wrong cmd\n");
		}
    }
}
